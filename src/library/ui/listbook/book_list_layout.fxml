<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane fx:id="rootView" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.ui.listbook.BookListController">
    <children>
        <TableView fx:id="tableView" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="titleColumn" prefWidth="75.0" text="Title"/>
                <TableColumn fx:id="idColumn" prefWidth="75.0" text="BookID"/>
                <TableColumn fx:id="authorColumn" prefWidth="75.0" text="Author"/>
                <TableColumn fx:id="publisherColumn" prefWidth="75.0" text="Publisher"/>
                <TableColumn fx:id="isbnColumn" prefWidth="75.0" text="ISBN"/>
                <TableColumn fx:id="availableColumn" prefWidth="75.0" text="Available"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>
